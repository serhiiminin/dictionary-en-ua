{
  "name": "my-dictionary",
  "version": "0.1.0",
  "private": true,
  "homepage": "/dictionary",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "now-build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "test": "react-app-rewired test --env=jsdom --watchAll",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "lint": "eslint ./ --ext '.js,.jsx' --max-warnings 0 $LINT_ARGS && echo 'Your code is clear' || (echo 'Please, fix problems'; exit 1)",
    "lint:fix": "LINT_ARGS=--fix yarn run lint",
    "lint:with-cache": "LINT_ARGS='--cache --cache-location=.cache/' yarn run lint",
    "format": "prettier --write 'src/**/*.js'",
    "validate": "yarn run lint"
  },
  "dependencies": {
    "@material-ui/core": "4.1.1",
    "@material-ui/icons": "4.2.0",
    "formik": "1.5.7",
    "js-cookie": "2.2.0",
    "moment": "2.24.0",
    "notistack": "0.8.6",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-facebook-login": "4.1.1",
    "react-google-login": "5.0.4",
    "react-router-dom": "5.0.1",
    "react-router-prop-types": "1.0.4",
    "react-scripts": "3.0.1",
    "recompose": "0.30.0",
    "styled-components": "4.3.1",
    "typescript": "3.5.2",
    "url-joiner": "1.1.3",
    "uuid": "3.3.2",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.10.1",
    "customize-cra": "0.2.13",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.4.1",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/styles.js",
      "!src/index.js",
      "!src/serviceWorker.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run validate"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
