{
  "name": "my-dictionary",
  "version": "0.1.0",
  "private": true,
  "homepage": "/dictionary",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "now-build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "test": "react-app-rewired test --env=jsdom --watchAll",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "lint": "eslint ./ --ext '.js,.jsx,.ts,.tsx' --max-warnings 0 $LINT_ARGS",
    "lint:fix": "LINT_ARGS=--fix yarn run lint",
    "lint:with-cache": "LINT_ARGS='--cache --cache-location=.cache/' yarn run lint",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "validate": "yarn run lint",
    "commit": "git-cz"
  },
  "dependencies": {
    "@hapi/boom": "7.4.2",
    "@material-ui/core": "4.2.1",
    "@material-ui/icons": "4.2.1",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/hapi__boom": "7.4.0",
    "@types/jest": "24.0.15",
    "@types/js-cookie": "2.2.2",
    "@types/material-ui": "0.21.6",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-facebook-login": "4.1.0",
    "@types/react-router-dom": "4.3.4",
    "@types/recompose": "0.30.6",
    "@types/styled-components": "4.1.18",
    "@types/yup": "0.26.22",
    "formik": "1.5.8",
    "js-cookie": "2.2.0",
    "notistack": "0.8.8",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-facebook-login": "4.1.1",
    "react-google-login": "5.0.4",
    "react-router-dom": "5.0.1",
    "react-router-prop-types": "1.0.4",
    "react-scripts": "3.0.1",
    "recompose": "0.30.0",
    "styled-components": "4.3.2",
    "typescript": "3.5.3",
    "url-joiner": "1.1.3",
    "uuid": "3.3.2",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "babel-plugin-styled-components": "1.10.6",
    "chalk": "2.4.2",
    "commitizen": "4.0.1",
    "customize-cra": "0.4.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "husky": "3.0.1",
    "lint-staged": "9.2.0",
    "longest": "2.0.1",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "word-wrap": "1.2.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/styles.js",
      "!src/index.js",
      "!src/serviceWorker.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn run validate"
    }
  },
  "config": {
    "commitizen": {
      "path": "./commitizen.config.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
